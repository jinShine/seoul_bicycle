  pod 'RxSwift', '~> 4.0'
  pod 'RxCocoa', '~> 4.0'
  pod 'RxDataSources', '~> 3.0'
  
  
  # Network
  pod 'Moya', '~> 13.0.1'
  pod 'Moya/RxSwift', '~> 13.0.1'
  
  
  # image
  pod 'Kingfisher', '~> 4.0'


  # Firebase
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'Firebase/Analytics'
  pod 'Firebase/Messaging'
  pod 'Firebase/Auth'
  
  
  # Custom View
  pod 'Toast-Swift', '~> 5.0.0' # Toast
  pod 'NVActivityIndicatorView' # ActivityIndicator
  
  # Utilities
  pod 'KeychainAccess'

end
